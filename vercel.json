{
    "version": 2,
    "builds": [
      {
        "src": "portfolio/package.json",
        "use": "@vercel/react"
      },
      {
        "src": "Backend/Server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/sendMail/(.*)",
      "dest": "/Backend/$1"
      },
      {
        "src": "/(.*)",
      "dest": "/portfolio/$1"
      }
    ]
  }
  